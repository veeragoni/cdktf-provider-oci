{
  "provider": {
    "name": "oracle/oci",
    "version": "~> 6.0",
    "source": "hashicorp/terraform-provider-oci"
  },
  "cdktf": {
    "version": "^0.20.0"
  },
  "languages": {
    "typescript": {
      "enabled": true,
      "packageName": "cdktf-provider-oci",
      "packageDescription": "CDKTF bindings for Oracle Cloud Infrastructure"
    },
    "python": {
      "enabled": true,
      "packageName": "cdktf-provider-oci",
      "moduleName": "cdktf_provider_oci"
    },
    "java": {
      "enabled": false,
      "packageName": "com.hashicorp.cdktf.providers.oci"
    },
    "csharp": {
      "enabled": false,
      "packageName": "HashiCorp.Cdktf.Providers.Oci"
    },
    "go": {
      "enabled": false,
      "packageName": "github.com/veeragoni/cdktf-provider-oci-go"
    }
  },
  "autoUpdate": {
    "enabled": true,
    "checkFrequency": "daily",
    "createPullRequest": true,
    "autoMerge": false
  }
}